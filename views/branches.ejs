            <div>
                <h1 class="branch__data-title">Sucursal <span><%= data[i].sucursal %></span></h1>
                <h2 class="branch__data-subtitle">Telefono <span><%= data[i].telefono %></span></h2>
                <h2 class="branch__data-subtitle">Direcci√≥n <span><%= data[i].direccion %></span></h2>
            </div>
        </div>
    <% } %>
    </section>
    <%- include('partials/backhome.ejs') %>
</body>
</html>